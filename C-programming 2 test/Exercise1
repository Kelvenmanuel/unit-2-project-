#include <stdio.h>
// This variable is for all the numbers that the user is going to write(input)
int number[5];
int num;
// this variable is for the calculation of the cube of all inputs 
int cube;

int main(void) {
   
  printf("Please Enter numbers\n");
  for (int i=0; i<5; i++){
    scanf("%i", &num);
    number[i]=num;
    
  }
  for (int i=0; i<5; i++){
  
  cube = number[i]*number[i]*number[i];  
  printf("Number is: %i and the cube of the %i is: %i\n",number[i], number[i], cube );

  }
  return 0;
}
