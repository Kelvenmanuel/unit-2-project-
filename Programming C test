#include <stdio.h>

int main(void) {
int angle1;
int angle2;
int angle3;
int triangle;

  printf("Enter three angles\n");
  scanf("%i %i %i", &angle1, &angle2, &angle3);
  triangle = angle1 + angle2 + angle3;

  if (triangle >= 180 ){
    printf("The triangle is valid\n");

  } else {

    printf("The triangle is not valid\n");
    }

  return 0;
}

