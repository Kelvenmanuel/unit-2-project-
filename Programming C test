#include <stdio.h>


int main(void) {
  int number;
  int count;
  printf("Enter the number\n");

  scanf ("%d", &number);

  if (number % 2 == 0){
    
    printf("%d is and even number\n", number);

  }else{
     
   printf("%d is and odd number\n", number);
  }
  return 0;
}

