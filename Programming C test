 #include <stdio.h>

int main(void) {
  int customerID=0;
  char name[100];
  int unitConsumed=0;
  float surcharge=0;
  float perUnit=0;
  float Amount=0;
  float Value;

  printf(" Enter your data : Customer ID, Name, Unit Consumed.\n");
  scanf("%i %s %i", &customerID, name, &unitConsumed);

  
   if (unitConsumed > 0 && unitConsumed < 200) {
     Value =unitConsumed*1.20;
    perUnit=12/10;
  }
  else if (unitConsumed >= 200 && unitConsumed < 400) {
    Value =unitConsumed*1.50;
    perUnit=15/10;
  }
  else if (unitConsumed >= 400 && unitConsumed < 600) {
    Value=unitConsumed*1.80;
    perUnit=18/10;
   }
   else if (unitConsumed >= 600) {
     Value = unitConsumed *2.00;
     perUnit=2.00;
   }
  if ( Value > 400) {
    surcharge= Value *15/100;
    Value=Value*115/100;

  } else if (unitConsumed < 100) {
    printf("Invalid data. Enter correct data\n");

  }
  
  Amount=Value-surcharge;

  printf("Customer ID: %i\n", customerID);
  printf("Customer Name: %s\n", name);
  printf("Unit Consumed: %i\n", unitConsumed);
  printf("Amount Charge: @R.s %.2f per unit: %.2f\n", perUnit,Amount);
  printf("Surcharge: %.2f\n", surcharge);
  printf("Net Amount Paid by the Customer: %.2f\n", Value);
  return 0;
}
