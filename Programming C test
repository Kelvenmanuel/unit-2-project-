#include <stdio.h>

int main(void) {
  
  int temperature;
  

  printf("Enter the temperature\n");

  scanf ("%i", &temperature);

  if ( temperature < 0){

  printf("Freezing weather\n"); 
  }
  for ( temperature = 0; temperature < 10; temperature++){
  printf("Very Cold Weather\n");
  }

  for ( temperature = 10; temperature < 20; temperature++){
  printf("Cold wheater\n");
  }

  
  for ( temperature = 20; temperature < 30; temperature++){
       
  printf("Normal in Temp\n");
  }
  
  for ( temperature = 30; temperature < 40; temperature++){
  printf("Normal in Temp\n");
  }

  if( temperature >= 40){
       printf("Its Very Hot\n");
  }

return 0;
}




