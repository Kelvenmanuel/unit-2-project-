#include <stdio.h>

int main(void) {
  int math;
  int phy;
  int chem;
  int total3subjects;
  int totalmathphy;

  printf("Enter 1st Phy marks, 2nd Chem marks and 3rd Math numbers\n");

  scanf ("%i %i %i", &phy, &chem, &math);
  total3subjects = math + phy + chem;
  totalmathphy = math + phy;

  if (math >= 65 && phy >= 55 && chem >= 50 && total3subjects >= 180){

    printf("The candidate is eligible for admission"); 
    
  } else {
     printf("The candidate is not eligible for admission");
    }   
return 0;
}

