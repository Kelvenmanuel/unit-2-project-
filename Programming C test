#include <stdio.h>


int main(void) {
  int number1; 
  int number2;
  int number3;

  printf("Enter three numbers\n");

  scanf ("%i %i %i", &number1, &number2, &number3);

  if (number1 > number2 && number1 > number3){
    
    printf("1st number = %i, 2nd number = %i, 3rd number = %i \n", number1, number2, number3);
    printf("the 1st Number is the greatest among three ");

  } else if (number2 > number1 && number2 > number3){

    printf("1st number = %i, 2nd number = %i, 3rd number = %i \n", number1, number2, number3);
    printf("the 2nd Number is the greatest among three ");

    } else if (number3 > number1 && number3 > number2){

      printf("1st number = %i, 2nd number = %i, 3rd number = %i \n", number1, number2, number3);
      printf("the 3rd Number is the greatest among three ");

     }
    return 0;

  }
  
