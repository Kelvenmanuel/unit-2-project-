#include <stdio.h>

int main(void) {
  
 int angle1;
 int angle2;
 int angle3;
 printf("Enter the angles of the triangle\n");
 scanf("%i %i %i", &angle1, &angle2, &angle3);
 
 if (angle1 == angle2 && angle1 == angle3 && angle2 == angle3 ){
    printf("This triangle is an Equilateral");

  } else if( angle1 == angle2 && angle1 != angle3 && angle2 != angle3){
     printf("This triangle is an Isosceles\n");

    } else if(angle1 != angle2 && angle1 != angle3 && angle2 != angle3){

        printf("This triangle is an Scalene\n"); 

      }
    
return 0;
}

